<!doctype html>
<html>
  <head>
    <title>opencnam Lookup</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootswatch/3.2.0/readable/bootstrap.min.css" rel="stylesheet">
  </head>
  <body>
    <div>
      <span>
        Caller ID for <%= recognitionResults['com.solveforall.recognition.phone.UsPhoneNumber'][0].fullNumber %>:
      </span>
      &nbsp;
      <span id="caller_id">
        <i>Loading ...</i>
      </span>
      <br/>
      <br/>
      <div>
        <small>
          Data provided by <a href="https://www.opencnam.com/" target="_top">opencnam</a>.
          <span id="opencnam_tier_info"></span>
        </small>
      </div>
    </div>

    <div id="info_holder"
     data-settings="<%= JSON.stringify(context.settings) %>"
     data-client-kind="<%= context.client.kind %>"
     data-full-number="<%= recognitionResults['com.solveforall.recognition.phone.UsPhoneNumber'][0].fullNumber %>">
    </div>
        
    <script src="https://cdn.jsdelivr.net/g/jquery@1.11.1,underscorejs@1.6.0,underscore.string@2.3.3,bootstrap@3.2.0">
    </script>
    
    <script src="https://dl.dropboxusercontent.com/u/34577952/solveforall/plugins/opencnam/lookup.js"></script>
  </body>
</html>
